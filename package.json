    "browser-sync": "^2.18.12",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^3.5.0",
